{
  "manifest_version": 2,

  "name": "Alltab Reloader",
  "description": "Extension for reloading inactive tabs on Google Chrome",
  "version": "0.1",

  "browser_action": {
   "default_title": "Reload all"
  },

  "background": {
        "scripts": [
            "background.js"
        ]
    },

  "permissions": [
   "activeTab",
   "contextMenus",
   "tabs"
   ]
}
